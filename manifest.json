{
  "manifest_version": 3,
  "name": "HN Link Finder",
  "version": "1.0",
  "description": "Finds and extracts links from Hacker News threads",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["https://news.ycombinator.com/*"],
  "action": {
    "default_popup": "popup.html"
  }
}
